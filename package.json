{
  "name": "Evalix",
  "version": "0.3.1",
  "description": "Calculateur Evalix",
  "main": "./out/main/index.js",
  "author": "JVTP SNC",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "publish:only": "electron-builder --publish always",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "publish": "electron-vite build && electron-builder --mac --publish always && electron-builder --win --config --ia32 --x64 --publish always",
    "generate-icons": "electron-icon-builder --input=./resources/icon.png --output=./build/icons"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@supabase/supabase-js": "2.48.1",
    "date-fns": "4.1.0",
    "electron-store": "10.0.1",
    "electron-updater": "^6.3.9",
    "lodash": "4.17.21",
    "lottie-react": "2.4.1",
    "motion": "12.0.5",
    "node-machine-id": "1.1.12",
    "react-hook-form": "7.54.2",
    "react-icons": "5.4.0",
    "react-router": "7.1.1",
    "vite-plugin-electron": "0.29.0",
    "vite-plugin-electron-renderer": "0.14.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^25.1.8",
    "electron-icon-builder": "2.0.1",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "supabase": "2.9.6",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a"
}